spring.application.name=phoenix
server.port=8999
server.context-path=/phoenix


# phoenix 数据源自定义配置
phoenix.enable= true
phoenix.url=jdbc:phoenix:192.168.20.6,192.168.20.8:2181:/hbase-unsecure
phoenix.type=com.alibaba.druid.pool.DruidDataSource
phoenix.driver-class-name=org.apache.phoenix.jdbc.PhoenixDriver
phoenix.username=
